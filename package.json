{
  "name": "api-mock-runner",
  "version": "0.0.0",
  "description": "Create mocks servers with its schemas",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "dist-types",
    "src",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node .",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "format": "npm run format:eslint",
    "format:eslint": "npm run lint:eslint -- --fix",
    "prepare": "npx husky install",
    "types": "tsc --build --pretty",
    "prepublishOnly": "npm run types",
    "release": "npx semantic-release"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^3.1.2",
    "open-api-mocker": "github:adantoscano/open-api-mocker"
  },
  "devDependencies": {
    "@os3/commitlint-config": "^0.0.3",
    "@os3/eslint-config": "^0.2.1",
    "@os3/lint-staged-config": "^0.0.4",
    "@os3/semantic-release-config": "^0.1.1",
    "@os3/tsconfig": "^0.0.4",
    "husky": "^8.0.0"
  }
}
